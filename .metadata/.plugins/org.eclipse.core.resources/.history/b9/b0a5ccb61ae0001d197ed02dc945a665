		List<Integer> input = new ArrayList<>();
        input.add(0);
        input.add(2);
        input.add(0);
        input.add(1);
        input.add(0);
        input.add(3);

        List<Integer> output = new ArrayList<>();
        for (int i = 0; i < input.size(); i++) {
            int element = input.get(i);
            if (element == 0) {
                output.add(0);
            } else {
                output.add(i - output.lastIndexOf(0), element);
            }
